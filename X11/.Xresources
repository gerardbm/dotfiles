! ------------------------------------------------------------
! URxvt plugins
! ------------------------------------------------------------

! Perl extensions
URxvt.perl-ext             : default,url-select,resize-font,unichr

! Plugin: url-select
URxvt.keysym.C-M-u         : perl:url-select:select_next
URxvt.url-select.launcher  : tmux new-window w3m
URxvt.url-select.underline : true
!
! Pressing 'y', the URL is copied to PRIMARY
! To paste it, press Shift-Insert
!
! Or...
!
! Copy it from PRIMARY to clipboard with C-M-c
! Now it can be pasted from clipboard with C-M-v
!
URxvt.keysym.Control-M-v   : eval:paste_clipboard
URxvt.keysym.Control-M-c   : eval:selection_to_clipboard

! Plugin: resize-font
URxvt.keysym.Control-Down  : resize-font:smaller
URxvt.keysym.Control-Up    : resize-font:bigger
URxvt.keysym.Control-Left  : resize-font:reset

! ------------------------------------------------------------
! XTerm
! ------------------------------------------------------------

xterm*termName        : xterm-256color
xterm*renderFont      : true
xterm*faceName        : DejaVu Sans Mono for Powerline
xterm*faceSize        : 11
xterm*locale          : true
xterm*metaSendsEscape : true
xterm*eightBitInput   : false
xterm*eightBitControl : false
xterm*eightBitOutput  : true
xterm*loginShell      : true
xterm*scrollBar       : false
xterm*saveLines       : 4096
xterm*utf8            : 2
xterm*jumpScroll      : true
xterm*multiScroll     : true
xterm*trimSelection   : true

xterm*background      : base1
xterm*foreground      : base4
xterm*cursorColor     : base4
xterm*cursorUnderline : true
xterm*cursorBlink     : false
xterm*dynamicColors   : true
xterm*boldMode        : true
xterm*colorBDMode     : false

xterm*VT100.Translations: #override \
	Ctrl Alt <Key>V: insert-selection(CLIPBOARD) \n\
	Ctrl Alt <Key>C: copy-selection(CLIPBOARD) \n\

xterm*charClass: 33:48,37-38:48,45-47:48,64:48,58:48,126:48,61:48,63:48,43:48,35:48

! ------------------------------------------------------------
! Rofi
! ------------------------------------------------------------

rofi.terminal          : /usr/bin/urxvtc
rofi.run-shell-command : /usr/bin/urxvtc -e '{cmd}'
rofi.ssh-command       : tmux split-window -h -p 50 '{ssh-client} {host}'
rofi.sidebar-mode      : true
rofi.fuzzy             : true
rofi.font              : Terminus\ 12
rofi.location          : 0
rofi.width             : 34
rofi.padding           : 8
rofi.borderwidth       : 1
rofi.columns           : 1
rofi.lines             : 10
rofi.separator-style   : solid

! Key bindings
rofi.kb-accept-entry        : Control+m,Return,KP_Enter
rofi.kb-cancel              : Escape,Control+c
rofi.kb-mode-next           : Control+l
rofi.kb-mode-previous       : Control+h
rofi.kb-row-down            : Control+j
rofi.kb-row-up              : Control+k
rofi.kb-remove-to-eol       : Control+y
rofi.kb-remove-to-sol       : Control+u
rofi.kb-remove-char-back    : Control+s,BackSpace
rofi.kb-remove-char-forward : Control+d,Delete

! Colors            bg     fg     altbg  hlbg   hlfg
rofi.color-normal : base1, base4, base1, base3, base1
rofi.color-urgent : base1, acr50, base1, acr50, base1
rofi.color-active : base1, acc50, base1, acc50, base1
rofi.color-window : base1, base3, base3
rofi.bw           : 1

! ------------------------------------------------------------
! Information
! ------------------------------------------------------------

! XTerm escape sequences
! 10;  = base4 (foreground)
! 11;  = base1 (background)
! 12;  = base4 (cursorColor)
! 708; = base1 (borderColor)
! 4;1  = acr50
! 4;2  = acg50
! 4;3  = aco50
! 4;4  = acb50
! 4;5  = acv50
! 4;6  = acc50
! 4;7  = base3
! 4;8  = base2
! 4;9  = acr70
! 4;10 = acg70
! 4;11 = aco70
! 4;12 = acb70
! 4;13 = acv70
! 4;14 = acc70
! 4;15 = base4
! 710; = set normal font
! 711; = set bold font
! 712; = set italic font
! 713; = set bold italic font
